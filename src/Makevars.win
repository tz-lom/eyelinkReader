.PHONY: all edfapi

all: $(SHLIB)
$(SHLIB): edfapi

clean:
	rm ./*.dll


ifeq "$(WIN)" "64"
PKG_LIBS = -L./edfapi/win64 -ledfapi64
edfapi:
	cp ./edfapi/win64/*.dll ./

else
PKG_LIBS = -L./edfapi/win32 -ledfapi
edfapi:
	cp ./edfapi/win32/*.dll ./

endif

PKG_CPPFLAGS = -I./edfapi/headers

